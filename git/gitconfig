[pull]
    rebase = true
[alias]
    co = checkout
    pom = push origin master
    br = branch
    ci = commit
    amend = commit --amend
    st = status
    ll = log --graph --branches --decorate --oneline
    lll = log --graph --branches --decorate
    last = log -1 HEAD
    unstage = reset HEAD --
[push]
    default = simple
[color]
    ui = true
[help]
    autocorrect = 1
[core]
    editor = /usr/bin/vim
[filter "media"]
	clean = git media clean %f
	smudge = git media smudge %f
	required = true
